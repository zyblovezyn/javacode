<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">


    <dubbo:application name="user-service-provide"/>
    <dubbo:registry address="zookeeper://192.168.209.129:2181"></dubbo:registry>
    <dubbo:protocol name="dubbo" port="20080"></dubbo:protocol>
    <dubbo:service interface="com.luoyiguren.dubbo.service.UserService" ref="userServieImpl" />

    <bean id="userServieImpl" class="com.luoyiguren.dubbo.service.impl.UserServiceImpl"></bean>
</beans>